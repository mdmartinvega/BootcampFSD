Algoritmo sin_titulo
	
	Definir filas, columnas, n_filas, contador Como Entero;
	
	Leer n_filas;
	contador = 1;
	
	Para filas = 1 Hasta n_filas Con Paso 1 Hacer
		
		
		Para columnas = 1 Hasta filas Con Paso 1 Hacer
			
			Escribir contador " "; sin saltar;
			contador = contador + 1;
			
		Fin Para
		
		Escribir "";
		
	Fin Para
	
	
FinAlgoritmo
